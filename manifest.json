{
    "manifest_version": 3,
    "name": "Stock gain loss calculator",
    "version": "1.1.2",
    "description": "Calculate stock gain/loss from history transactions",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/logo-16.png",
        "48": "icons/logo-48.png",
        "128": "icons/logo-128.png"
      }
    },
    "icons": {
      "16": "icons/logo-16.png",
      "48": "icons/logo-48.png",
      "128": "icons/logo-128.png"
    },
    "permissions": [
      "storage"
    ]
  }
    